{
	"name": "yourflix",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"analyze": "ANALYZE=true yarn build",
		"build": "next build",
		"build-storybook": "storybook build",
		"commit": "commit",
		"db:migrate": "dotenv -e .env.local prisma migrate dev",
		"dev": "next dev",
		"dev:docker": "concurrently --kill-others-on-fail \"npm run start:db\" \"npm run start:dev\"",
		"dev:storybook": "storybook dev -p 6006",
		"format": "prettier -w .",
		"lint": "next lint --fix --cache && npm run format",
		"lint-staged": "lint-staged",
		"prepare": "husky install",
		"start": "next start",
		"start:db": "docker compose up",
		"start:dev": "wait-on tcp:5432 -i 1000 && npm run db:migrate && next dev",
		"test": "vitest",
		"typecheck": "tsc --noEmit --incremental false"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.15",
		"@hookform/resolvers": "^3.1.1",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^4.16.0",
		"@tanstack/react-query": "^4.29.15",
		"@trpc/client": "^10.31.0",
		"@trpc/next": "^10.31.0",
		"@trpc/react-query": "^10.31.0",
		"@trpc/server": "^10.31.0",
		"@zodios/core": "^10.9.2",
		"axios": "^1.4.0",
		"bcrypt": "^5.1.0",
		"clsx": "^1.2.1",
		"dayjs": "^1.11.8",
		"next": "^13.4.7",
		"next-auth": "^4.22.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.45.0",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^4.10.1",
		"sharp": "^0.32.1",
		"superjson": "^1.12.3",
		"swiper": "^9.4.1",
		"tailwind-merge": "^1.13.2",
		"tailwindcss-animate": "^1.0.6",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@commitlint/prompt-cli": "^17.6.5",
		"@next/bundle-analyzer": "^13.4.7",
		"@storybook/addon-essentials": "7.0.22",
		"@storybook/addon-interactions": "7.0.22",
		"@storybook/addon-links": "7.0.22",
		"@storybook/blocks": "7.0.22",
		"@storybook/nextjs": "7.0.22",
		"@storybook/react": "7.0.22",
		"@storybook/testing-library": "0.2.0",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/bcrypt": "^5.0.0",
		"@types/eslint": "^8.40.2",
		"@types/node": "^18.16.18",
		"@types/react": "^18.2.13",
		"@vitejs/plugin-react": "4.0.1",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.43.0",
		"eslint-config-next": "^13.4.7",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-tailwindcss": "^3.12.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-jsdoc": "^0.4.2",
		"prettier-plugin-packagejson": "^2.4.3",
		"prettier-plugin-prisma": "^4.13.0",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.16.0",
		"storybook": "7.0.22",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.1.3",
		"vitest": "^0.32.2",
		"wait-on": "^7.0.1"
	},
	"engines": {
		"node": "^18.x.x",
		"npm": "^9.x.x"
	}
}
