{
	"name": "xmdb",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"engines": {
		"node": "^18.x.x",
		"npm": "^10.x.x"
	},
	"scripts": {
		"analyze": "ANALYZE=true yarn build",
		"build": "next build",
		"build-storybook": "storybook build",
		"commit": "commit",
		"db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
		"db:push": "dotenv -e .env.production.local -- npx prisma db push",
		"db:studio": "dotenv -e .env.production -- npx prisma studio",
		"dev": "next dev",
		"dev:docker": "concurrently --kill-others-on-fail \"npm run start:db\" \"npm run start:dev\"",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"format": "prettier -w .",
		"knip": "knip",
		"lint": "next lint --fix --cache && npm run format",
		"lint-staged": "lint-staged",
		"postinstall": "prisma generate",
		"prepare": "husky install",
		"start": "next start",
		"start:db": "docker compose up",
		"start:dev": "wait-on tcp:5432 -i 1000 && npm run db:migrate && next dev",
		"test": "vitest",
		"typecheck": "tsc --noEmit --incremental false"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@hookform/resolvers": "^3.3.1",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^4.16.2",
		"@tanstack/react-query": "^4.35.3",
		"@trpc/client": "^10.38.4",
		"@trpc/next": "^10.38.4",
		"@trpc/react-query": "^10.38.4",
		"@trpc/server": "^10.38.4",
		"@zodios/core": "^10.9.6",
		"axios": "^1.5.1",
		"bcrypt": "^5.1.1",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.10",
		"next": "^13.5.3",
		"next-auth": "^4.23.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.46.2",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^4.11.0",
		"sharp": "^0.32.6",
		"superjson": "^1.13.1",
		"swiper": "^10.3.0",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@commitlint/prompt-cli": "^17.7.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.0",
		"@next/bundle-analyzer": "^13.5.3",
		"@storybook/addon-essentials": "^7.4.5",
		"@storybook/addon-interactions": "^7.4.5",
		"@storybook/addon-links": "^7.4.5",
		"@storybook/blocks": "^7.4.5",
		"@storybook/nextjs": "^7.4.5",
		"@storybook/react": "^7.4.5",
		"@storybook/testing-library": "^0.2.1",
		"@testing-library/react": "^14.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/eslint": "^8.44.3",
		"@types/node": "^20.7.0",
		"@types/react": "^18.2.23",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"@vitejs/plugin-react": "^4.1.0",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.1",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.50.0",
		"eslint-config-next": "^13.5.3",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.14",
		"eslint-plugin-tailwindcss": "^3.13.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"knip": "^2.29.0",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.30",
		"prettier": "^3.0.3",
		"prettier-plugin-jsdoc": "^1.0.2",
		"prettier-plugin-pkg": "^0.18.0",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-sh": "^0.13.1",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"prisma": "^4.16.2",
		"storybook": "^7.4.5",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.2.2",
		"vitest": "^0.34.5",
		"wait-on": "^7.0.1"
	}
}
