{
	"name": "xmdb",
	"version": "1.3.0",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"analyze": "ANALYZE=true npm run build",
		"build": "next build",
		"build:storybook": "storybook build",
		"build:turbo": "npx turbo build",
		"check:turbo": "npx turbo lint type-check",
		"commit": "commit",
		"db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
		"db:push": "dotenv -e .env.production.local -- npx prisma db push",
		"db:studio": "dotenv -e .env.production -- npx prisma studio",
		"dev": "next dev",
		"dev:docker": "concurrently --kill-others-on-fail \"npm run start:db\" \"npm run start:dev\"",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"format": "prettier -w .",
		"knip": "knip",
		"lint": "next lint --fix --cache && npm run format",
		"lint-staged": "lint-staged",
		"postinstall": "prisma generate",
		"prepare": "husky install",
		"push-release": "git push --follow-tags origin main",
		"release": "dotenv -e HUSKY=0 commit-and-tag-version",
		"start": "next start",
		"start:db": "docker compose up",
		"start:dev": "wait-on tcp:5432 -i 1000 && npm run db:migrate && next dev",
		"test": "vitest",
		"type-check": "dotenv -e SKIP_ENV_VALIDATION=false tsc --noEmit"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^1.0.11",
		"@headlessui/react": "^1.7.17",
		"@hookform/resolvers": "^3.3.2",
		"@prisma/client": "^5.7.0",
		"@tanstack/react-query": "^5.14.1",
		"@tanstack/react-query-devtools": "^5.14.1",
		"@tanstack/react-query-next-experimental": "^5.14.1",
		"@trpc/client": "11.0.0-alpha-next-2023-12-15-18-34-32.118",
		"@trpc/next": "11.0.0-alpha-next-2023-12-15-18-34-32.118",
		"@trpc/react-query": "11.0.0-alpha-next-2023-12-15-18-34-32.118",
		"@trpc/server": "11.0.0-alpha-next-2023-12-15-18-34-32.118",
		"@zodios/core": "^10.9.6",
		"axios": "^1.6.2",
		"bcryptjs": "^2.4.3",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.10",
		"next": "^14.0.4",
		"next-auth": "5.0.0-beta.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.49.2",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^4.12.0",
		"sharp": "^0.33.0",
		"superjson": "^2.2.1",
		"tailwind-merge": "^2.1.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@commitlint/cz-commitlint": "^18.4.3",
		"@commitlint/prompt-cli": "^18.4.3",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.1",
		"@next/bundle-analyzer": "^14.0.4",
		"@storybook/addon-essentials": "^7.6.5",
		"@storybook/addon-interactions": "^7.6.5",
		"@storybook/addon-links": "^7.6.5",
		"@storybook/blocks": "^7.6.5",
		"@storybook/nextjs": "^7.6.5",
		"@storybook/react": "^7.6.5",
		"@storybook/testing-library": "^0.2.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/react": "^14.1.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^8.44.8",
		"@types/node": "^20.10.4",
		"@types/react": "^18.2.45",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.16",
		"commit-and-tag-version": "^12.0.0",
		"commitizen": "^4.3.0",
		"commitlint": "^18.4.3",
		"concurrently": "^8.2.2",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.56.0",
		"eslint-config-next": "^14.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-tailwindcss": "^3.13.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jsdom": "^23.0.1",
		"knip": "^3.8.1",
		"lint-staged": "^15.2.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-jsdoc": "^1.1.1",
		"prettier-plugin-pkg": "^0.18.0",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-sh": "^0.13.1",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"prisma": "^5.7.0",
		"storybook": "^7.6.5",
		"tailwind-scrollbar": "^3.0.5",
		"tailwindcss": "^3.3.6",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.3.3",
		"vitest": "^1.0.4",
		"wait-on": "^7.2.0"
	}
}
