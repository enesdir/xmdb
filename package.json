{
	"name": "xmdb",
	"version": "1.1.0",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"analyze": "ANALYZE=true npm run build",
		"build": "next build",
		"build:storybook": "storybook build",
		"build:turbo": "npx turbo build",
		"check:turbo": "npx turbo lint type-check",
		"commit": "commit",
		"db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
		"db:push": "dotenv -e .env.production.local -- npx prisma db push",
		"db:studio": "dotenv -e .env.production -- npx prisma studio",
		"dev": "next dev",
		"dev:docker": "concurrently --kill-others-on-fail \"npm run start:db\" \"npm run start:dev\"",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"format": "prettier -w .",
		"knip": "knip",
		"lint": "next lint --fix --cache && npm run format",
		"lint-staged": "lint-staged",
		"postinstall": "prisma generate",
		"prepare": "husky install",
		"push-release": "git push --follow-tags origin main",
		"release": "dotenv -e HUSKY=0 standard-version",
		"start": "next start",
		"start:db": "docker compose up",
		"start:dev": "wait-on tcp:5432 -i 1000 && npm run db:migrate && next dev",
		"test": "vitest",
		"type-check": "dotenv -e SKIP_ENV_VALIDATION=false tsc --noEmit"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@hookform/resolvers": "^3.3.2",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^4.16.2",
		"@tanstack/react-query": "^4.36.1",
		"@tanstack/react-query-devtools": "^4.36.1",
		"@trpc/client": "^10.44.1",
		"@trpc/next": "^10.44.1",
		"@trpc/react-query": "^10.44.1",
		"@trpc/server": "^10.44.1",
		"@zodios/core": "^10.9.6",
		"axios": "^1.6.2",
		"bcrypt": "^5.1.1",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.10",
		"next": "^14.0.3",
		"next-auth": "^4.24.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.48.2",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^4.12.0",
		"sharp": "^0.33.0",
		"superjson": "^2.2.1",
		"swiper": "^11.0.5",
		"tailwind-merge": "^2.0.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@commitlint/cz-commitlint": "^18.4.3",
		"@commitlint/prompt-cli": "^18.4.3",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.1",
		"@next/bundle-analyzer": "^14.0.3",
		"@storybook/addon-essentials": "^8.0.0-alpha.0",
		"@storybook/addon-interactions": "^8.0.0-alpha.0",
		"@storybook/addon-links": "^8.0.0-alpha.0",
		"@storybook/blocks": "^8.0.0-alpha.0",
		"@storybook/nextjs": "^8.0.0-alpha.0",
		"@storybook/react": "^8.0.0-alpha.0",
		"@storybook/testing-library": "^0.2.2",
		"@testing-library/react": "^14.1.2",
		"@types/bcrypt": "^5.0.2",
		"@types/eslint": "^8.44.8",
		"@types/node": "^20.10.1",
		"@types/react": "^18.2.39",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.16",
		"commitizen": "^4.3.0",
		"concurrently": "^8.2.2",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.54.0",
		"eslint-config-next": "^14.0.3",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-tailwindcss": "^3.13.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jsdom": "^23.0.1",
		"knip": "^3.3.4",
		"lint-staged": "^15.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-jsdoc": "^1.1.1",
		"prettier-plugin-pkg": "^0.18.0",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-sh": "^0.13.1",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"prisma": "^4.16.2",
		"standard-version": "^9.5.0",
		"storybook": "^8.0.0-alpha.0",
		"tailwindcss": "^3.3.5",
		"typescript": "^5.3.2",
		"vitest": "^0.34.6",
		"wait-on": "^7.2.0"
	}
}
